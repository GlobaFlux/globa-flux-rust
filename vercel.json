{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "env": {
    "CARGO_HOME": "/vercel/.cargo",
    "RUSTUP_HOME": "/vercel/.rustup"
  },
  "builds": [
    {
      "src": "api/**/*.rs",
      "use": "@vercel/rust",
      "config": {
        "maxDuration": 30
      }
    }
  ]
}
